{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "baseUrl": "./",
        "paths": {
            "@apis/*": [
                "./src/apis/*"
            ],
            "@assets/*": [
                "./src/assets/*"
            ],
            "@components/*": [
                "./src/components/*"
            ],
            "@interface/*": [
                "./src/interface/*"
            ],
            "@layout/*": [
                "./src/layout/*"
            ],
            "@pages/*": [
                "./src/pages/*"
            ],
            "@routes/*": [
                "./src/routes/*"
            ],
            "@shared/*": [
                "./src/shared/*"
            ],
            "@stores/*": [
                "./src/stores/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@/*": [
                "./src/*"
            ],
            "@context/*": [
                "./src/utils/context/*"
            ],
            "@Password/*": [
                "./src/pages/Password/*"
            ],
            "@AppPagination/*": [
                "./src/components/AppPagination/*"
            ]
        }
    },
    "include": [
        "src"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}